{
  "name": "@kentomoriwaki/react-apollo-typescript-boilerplate",
  "version": "0.1.0",
  "main": "src/index.js",
  "repository": "https://github.com/KentoMoriwaki/create-react-apollo-typescript/packages/react-apollo-typescript-boilerplate",
  "author": "KentoMoriwaki <kento@wantedly.com>",
  "license": "MIT",
  "bin": "./index.js",
  "scripts": {
    "start": "babel-node --no-babelrc --extensions .js,.ts --presets @babel/preset-env,@babel/preset-typescript src/index.js",
    "build": "webpack --run-prod",
    "serve": "NODE_ENV=production babel-node --no-babelrc --extensions .js,.ts --presets @babel/preset-env,@babel/preset-typescript src/server/index.ts",
    "tsc": "tsc --noEmit -p .",
    "update-api": "yarn run update-schema && yarn run update-types",
    "update-schema": "apollo-codegen introspect-schema http://localhost:8080/graphql --output schema.json",
    "update-types": "apollo-codegen generate src/client/** --schema schema.json --target typescript --output src/client/API.ts"
  },
  "dependencies": {
    "apollo-boost": "^0.1.6",
    "apollo-server-koa": "^1.3.6",
    "ejs": "^2.6.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "mz": "^2.7.0",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "v7.0.0-beta.49",
    "@babel/node": "7.0.0-beta.49",
    "@babel/preset-env": "v7.0.0-beta.49",
    "@babel/preset-react": "v7.0.0-beta.49",
    "@babel/preset-stage-3": "v7.0.0-beta.49",
    "@babel/preset-typescript": "v7.0.0-beta.49",
    "@types/ejs": "^2.5.1",
    "@types/koa": "^2.0.45",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.28",
    "@types/koa-static": "^4.0.0",
    "@types/mz": "^0.0.32",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "apollo-codegen": "^0.19.1",
    "babel-loader": "next",
    "chokidar": "^2.0.3",
    "typescript": "^2.8.3",
    "webpack": "^4.9.1",
    "webpack-command": "^0.2.0",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-serve": "^1.0.2"
  }
}
